{
  "name": "shopdev-fe-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate of front end.",
  "repository": "https://github.com/shopistan/react-admin-template",
  "author": "MUHAMMAD USMAN BUTT <usman.butt@shopdev.co>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "run-s dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "debug": "cross-env NODE_ENV=development webpack-dev-server --config webpack.debug.js",
    "build": "run-s lint build:no-lint",
    "test": "jest",
    "analyze": "run-s lint clean build:analyze",
    "build:no-lint": "run-s clean build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:analyze": "cross-env NODE_ENV=production webpack --config webpack.analyze.js",
    "clean": "rimraf dist",
    "lint": "run-p lint:js lint:style",
    "lint:js": "eslint \"**/*.js\"",
    "lint:style": "stylelint \"**/*.@(css|sass|scss)\"",
    "format": "run-p format:js format:style format:others",
    "format:js": "prettier-eslint --write \"**/*.js\"",
    "format:style": "run-s format:style:prettier format:style:stylelint",
    "format:others": "prettier --write \"**/*.@(json|gql|graphql)\"",
    "format:style:prettier": "prettier --write \"**/*.@(css|sass|scss)\"",
    "format:style:stylelint": "stylelint \"**/*.@(css|sass|scss)\" --fix"
  },
  "lint-staged": {
    "**/*.@(js|jsx)": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ],
    "**/*.@(css|sass|scss)": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "**/*.@(json|gql|graphql)": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.3.1",
    "axios": "^0.18.0",
    "faker": "^5.1.0",
    "formik": "^2.2.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "immer": "^2.1.5",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^26.5.2",
    "jest-enzyme": "^7.1.2",
    "js-cookie": "^2.2.0",
    "lodash-es": "^4.17.11",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-apollo": "^2.5.4",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "tiny-cookie": "^2.3.2",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.4.3",
    "@hot-loader/react-dom": "^16.8.6",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "extract-css-chunks-webpack-plugin": "^4.3.0",
    "fast-async": "^7.0.6",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.16.4",
    "prettier-eslint-cli": "^4.7.1",
    "react-dev-utils": "^8.0.0",
    "react-hot-loader": "^4.8.3",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-grpc-web-loader": "^0.1.4",
    "webpack-merge": "^4.2.1",
    "workerize-loader": "^1.0.4"
  },
  "browserslist": [
    "last 5 versions",
    "ie 11"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/libs/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
